                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Account" asp-action="Register">Реєстрація</a>
                        </li>
                        <li class=nav-item>
                            <a class="nav-link" asp-controller="Account" asp-action="Login">Ввійти</a>
                        </li>
                    </ul>
                }

            </div>
        </div>
